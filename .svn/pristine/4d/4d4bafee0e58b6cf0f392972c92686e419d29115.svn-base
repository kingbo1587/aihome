<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>七夕言情</title>
<link rel="stylesheet" href="style.css"/>
<link rel="stylesheet" href="pageA.css"/>
<link rel="stylesheet" href="pageB.css"/>
<link rel="stylesheet" href="pageC.css"/>
<script src="http://libs.baidu.com/jquery/1.9.0/jquery.js"></script>
<script src="jquery.transit.min.js"></script>
<script src="Qixi.js"></script>
<style type="text/css">
button {
	width: 100px;
	height: 50px;
}
.button {
	border: 1px solid red;
	position: absolute;
	bottom: 0;
}

</style>
</head>
<body>
<div id="content">
	<ul class="content-wrap">
		<!-- 第一副画面 -->
		<li>
			<div class="a_background">
				<div class="top"></div>
				<div class="middle"></div>
				<div class="bottom"></div>
			</div>
			<!-- 太阳 -->
			<div id="sun"></div>
			<!-- 云 -->
			<div id="cloudArea">
				<div class="cloud smallCloud"></div>
				<div class="cloud bigCloud"></div>
			</div>
		</li>
		<!-- 第二幅画面 -->
		<li>
			<!-- 背景 -->
			<div class="b_background"></div>
			<div class="b_background_preload"></div>
			<!-- 商店 -->
			<div class="shop">
				<div class="door">
					<div class="door-left"></div>
					<div class="door-right"></div>
				</div>
				<div class="lamp"></div>
			</div>
			<!-- 鸟 -->
			<div class="bird"></div>
		</li>
		<!-- 第三幅画面 -->
		<li>
			<!-- 背景 -->
			<div class="c_background">
				<div class="top"></div>
				<div class="middle"></div>
				<div class="bottom"></div>
			</div>
			<!-- 女孩 -->
			<div id="girl" class="charecter girl"></div>
			
			<!-- 水波 -->
			<div class="bridge-bottom">
				<ul class="water">
					<li id="water1" class="water1"></li>
					<li id="water2" class="water2"></li>
					<li id="water3" class="water3"></li>
					<li id="water4" class="water4"></li>
				</ul>
			</div>
			<!-- 星星 -->
			<ul class="stars">
				<li class="star1"></li>
				<li class="star2"></li>
				<li class="star3"></li>
				<li class="star4"></li>
				<li class="star5"></li>
				<li class="star6"></li>
			</ul>
			<!-- 慕课网logo图 -->
			<div class="logo"></div>
		</li>
	</ul>
	<div id="boy" class="charecter boy"></div>
	
	<!-- 飘花 -->
	<div id="snowflake"></div>
	
</div>
</body>
</html>